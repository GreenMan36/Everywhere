{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2021",
    "moduleResolution": "bundler",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "types": []
  },
  "include": ["./src"]
}
